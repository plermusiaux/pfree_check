CONSTRUCTORS

true : Bool
false : Bool

cons : Expr * List -> List
nil : List

bool : Bool -> Expr
list : List -> Expr

FUNCTIONS

flattenE : Expr -> Expr [- cons(list(l1),l2) ]
flattenL : List -> List [- cons(list(l1),l2) ]
concat : List * List -> List

RULES

flattenE(bool(b)) -> bool(b)
flattenE(list(l)) -> list(flattenL(l))

flattenL(cons(bool(b),l)) -> conc(bool(b),flattenL(l))
flattenL(cons(list(l1),l2)) -> concat(flatten(l1),flatten(l2))
flattenL(l) -> nil()

concat(cons(e,l1),l2) -> cons(e,concat(l1,l2))
concat(l1,l2) -> l2
